# Postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=dagster
POSTGRES_PORT=5432

# Dagster Web UI
DAGSTER_WEBSERVER_PORT=3000

# MLflow UI
MLFLOW_PORT=5000

# Qdrant API
QDRANT_PORT=6333

# LangFlow API (无头模式)
# 如使用 docker-compose 中的 langflow 服务，端口默认 7860
LANGFLOW_PORT=7860
# 可选：如果 LangFlow 启用了鉴权，请在本地 .env 提供 API Key（切勿提交真实密钥）
LANGFLOW_API_KEY=
# 可选：用于开发期减少镜像拉取延迟
LANGFLOW_IMAGE=langflowai/langflow:latest

# 镜像（可覆盖 compose 默认值）
USER_CODE_IMAGE=dagma/user-code:dev
DAGSTER_IMAGE=dagma/dagster:dev

# 可选：在公司/Clash 代理环境下启用构建代理（构建阶段使用）
# 例如：HTTP_PROXY=http://127.0.0.1:7890
HTTP_PROXY=
HTTPS_PROXY=
# 注意 NO_PROXY 包含容器内部互访主机名，避免走代理
# 常见包括：localhost,127.0.0.1,postgres,user_code,webserver,daemon,mlflow,qdrant
NO_PROXY=localhost,127.0.0.1,postgres,user_code,webserver,daemon,mlflow,qdrant

# ===== Minimal runtime config (MLFLOW/LANGFLOW/QDRANT) =====
# MLflow runtime toggle: set to true to enable real Tracking resource (otherwise Stub)
MLFLOW_USE_TRACKING=false
# Tracking endpoint
MLFLOW_TRACKING_URI=http://mlflow:5000
# Default experiment name
MLFLOW_EXPERIMENT=Default
# Optional: read-only credentials placeholder (if protected by gateway/proxy)
MLFLOW_USERNAME_READONLY=
MLFLOW_PASSWORD_READONLY=

# LangFlow REST base URL and API key
LANGFLOW_BASE_URL=http://localhost:${LANGFLOW_PORT:-7860}
LANGFLOW_API_KEY=
# Optional default flow id used by jobs
LANGFLOW_DEFAULT_FLOW_ID=

# Qdrant HTTP config
QDRANT_HOST=localhost
QDRANT_PORT=${QDRANT_PORT:-6333}
QDRANT_USE_HTTPS=false
QDRANT_API_KEY=
QDRANT_COLLECTION=embeddings
QDRANT_TIMEOUT=30
